   "source": [
    "from dataclasses import dataclass, asdict\n",
    "\n",
    "\n",
    "\n",
    "@dataclass\n",
    "class Test:\n",
    "    name: str\n",
    "    age: int\n",
    "    \n",
    "    def __post_init__(self):\n",
    "        assert self.age > 0, \"Age must be positive value\"\n",
    "        \n",
    "        \n",
    "a = Test(\"Son\", 2)\n",
    "\n",
    "b = asdict(a)\n",
    "a,b\n"
   ]
  }
